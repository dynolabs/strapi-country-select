{
  "version": "0.0.0",
  "type": "commonjs",
  "name": "strapi-country-select",
  "description": "A Custom Field plugin for Strapi to select countries in ISO 3166-1 alpha-2 format",
  "keywords": [
    "strapi",
    "strapi plugin",
    "custom fields",
    "country select",
    "ISO 3166"
  ],
  "homepage": "https://github.com/Dulajdeshan/strapi-country-select",
  "readme": "https://github.com/Dulajdeshan/strapi-country-select#readme",
  "bugs": {
    "url": "https://github.com/Dulajdeshan/strapi-country-select/issues",
    "email": "dulajdeshans@gmail.com"
  },
  "repository": {
    "url": "git+https://github.com/Dulajdeshan/strapi-country-select.git",
    "type": "git",
    "directory": "."
  },
  "author": {
    "name": "Dulaj Ariyaratne",
    "email": "dulajdeshans@gmail.com",
    "url": "https://github.com/dulajdeshan"
  },
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  },
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.12",
    "@strapi/icons": "^2.0.0-rc.12",
    "countries-list": "^3.1.1",
    "country-flag-icons": "^1.5.13",
    "react-intl": "^6.8.7"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.2.0",
    "@strapi/typescript-utils": "^5.2.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.3",
    "styled-components": "^6.1.13",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.3",
    "styled-components": "^6.1.13"
  },
  "strapi": {
    "kind": "plugin",
    "name": "strapi-country-select",
    "displayName": "Strapi Country Select",
    "description": "A Custom Field plugin for Strapi to select countries in ISO 3166-1 alpha-2 format"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "latest"
    ]
  },
  "license": "MIT"
}
